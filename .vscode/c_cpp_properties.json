{
    "configurations": [
        {
            "name": "Pico",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/common/pico_base_headers/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/common/pico_stdlib_headers/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/rp2_common/hardware_gpio/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/boards/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/rp2350/hardware_regs/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/rp2_common/hardware_base/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/common/hardware_claim/include",
                "${workspaceFolder}/build/release/_deps/pico-sdk-src/src/rp2_common/hardware_i2c/include",
                "${workspaceFolder}/build/release/generated/pico_base"
            ],
            "forcedInclude": [
                "${workspaceFolder}/build/release/generated/pico_base/pico/config_autogen.h"
            ],
            "defines": [
                "PICO_BOARD=\"pico2\"",
                "PICO_BUILD=1",
                "PICO_NO_FLASH=0",
                "PICO_ON_DEVICE=1",
                "PICO_RP2350=1"
            ],
            "compilerPath": "${workspaceFolder}/build/release/toolchain/arm-gnu-toolchain/bin/arm-none-eabi-gcc.exe",
            "compileCommands": "${workspaceFolder}/build/release/compile_commands.json",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-arm"
        }
    ],
    "version": 4
}
